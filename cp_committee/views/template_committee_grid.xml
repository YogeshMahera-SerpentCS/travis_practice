<odoo>

    <template id="template_committee_grid" name="Committees List">
        <t t-call="website.layout">
             <link rel="stylesheet"
                  href="/cp_committee/static/src/css/member_grid.css"/>
            <div class="grid_partner_bg"><br/><br/>
            <div class="container">
                <form name="frm">

                    <div class="row">
                        <div class="col-md-12">
                            <div class="panel panel-primary">
                                <div class="panel-heading">
                                    <h2 class="panel-title"><center>Committee Members and Their Positions</center></h2>
                                </div>
                                <table class="table table-condensed" id="dev-table">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Job Position</th>
                                            <th>Responsible Person</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <t t-foreach="job_pos_data" t-as="jobs">
                                        <tr>
                                            <td><span t-field="jobs.id"/></td>
                                            <td><span t-field="jobs.name"/></td>
                                            <td><span t-field="jobs.responsible_person_id.name"/></td>
                                        </tr>
                                        </t>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            </div>
        </t>
    </template>

</odoo>
